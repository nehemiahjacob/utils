#!/bin/bash

#=======================================================================
# strip-nonexisting
# File ID: 89302bf2-f33a-11e4-a2d5-000df06acc56
#
# Strip non-existing files from stdin
#
# Author: Ã˜yvind A. Holm <sunny@sunbase.org>
# License: GNU General Public License version 2 or later.
#=======================================================================

while read f; do
    test -e "$f" && echo "$f"
done
